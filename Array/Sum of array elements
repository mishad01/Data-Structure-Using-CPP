//SUM OF ARRAY
#include<bits/stdc++.h>
using namespace std;
int main (){
   int n;
   cin>>n;
   int a[n];
   int sum = 0;
   for(int i=0;i<n;i++){
    cin>>a[i];
   }
  /// printing
   for(int i = 0;i<n;i++){
    cout<<a[i]<<" ";
   }
   cout<<endl;
  for(int i =0 ;i<n;i++){
    sum += a[i];
  }
  cout<<sum<<endl;
}
